    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "^9.1.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "i18next": "^21.6.6"
